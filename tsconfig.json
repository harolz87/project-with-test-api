{
  "compilerOptions": {
    "incremental": true,
    "lib": ["es5", "es6", "dom"],
    "module": "commonjs",
    "jsx": "react",
    "sourceMap": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "alwaysStrict": true
  }
}
